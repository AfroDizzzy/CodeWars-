function sumStr(a,b) {
  numa = parseInt(a, 10);
  numb = parseInt(b, 10);
  nana = isNaN(numa);
  nanb = isNaN(numb);
  if (nana && nanb){
    return "0";
  }
  if (nana){
    return "" + numb;
  } 
  if (nanb){
    return "" + numa;
  } 
  return "" + (numa +numb);
}
