function getRealFloor(n) {
  
  switch (Math.sign(n)){
      case -1:
        return n;
      case 0:
        return 0;
      case 1:
        if (n >= 13){
          console.log(n);
          console.log(n-1);
          return n-2;
        }else if(n==1){
          return 0;
        } else {
          return n-1
        }
   }

}
